{
  "name": "nfl-fantasy-agent",
  "version": "0.1.0",
  "description": "A journalist agent that keeps the fun in NFL fantasy leagues",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env dist/index.js",
    "dev": "tsx --env-file=.env src/index.ts",
    "generate": "tsx --env-file=.env src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "nfl",
    "fantasy",
    "agent",
    "langgraph",
    "newsletter"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@langchain/anthropic": "^1.3.16",
    "@langchain/core": "^1.1.20",
    "@langchain/langgraph": "^1.1.4",
    "@tavily/core": "^0.7.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "vitest": "^4.0.18"
  }
}
